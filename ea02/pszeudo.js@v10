"use strict";function debuggerlike(c){this.elem=function(d){return $(this.melyik+d);return $(this.melyik+d)};this.jelol=function(){for(var d=1;this.elem(d)!=null;d++){classt_levesz(this.elem(d),"aktiv")}if(this.aktualis!=0){classt_feltesz(this.elem(this.aktualis),"aktiv")}};this.kovetkezo=function(){this.aktualis=this.csinal();if(this.elem(this.aktualis)==null){this.aktualis=0}this.jelol()};this.melyik=c;this.aktualis=0;this.jelol();var b=this.elem("kovetkezo"),a=this;addEvent(b,"click",function(d){a.kovetkezo()})}function szekvobj(a){debuggerlike.call(this,a);this.csinal=function(){switch(this.aktualis){case 0:this.elem("screenshot").innerHTML="\n\n\n";this.elem("a").innerHTML="";this.a=Math.round(Math.random()*8+2);return 1;case 1:this.elem("screenshot").innerHTML="Kérem a számot!\n<blink>_</blink>\n\n";return 2;case 2:this.elem("screenshot").innerHTML="Kérem a számot!\n"+this.a+"\n\n";this.elem("a").innerHTML="<strong>"+this.a+"</strong>";return 3;case 3:this.elem("a").innerHTML="<strong>"+this.a*this.a+"</strong>";return 4;case 4:this.elem("screenshot").innerHTML="Kérem a számot!\n"+this.a+"\n"+this.a*this.a+"\n";this.elem("a").innerHTML=this.a*this.a;return 0}}}function feltbobj(a){debuggerlike.call(this,a);this.csinal=function(){switch(this.aktualis){case 0:this.elem("screenshot").innerHTML="<blink>_</blink>\n\n\n";this.elem("a").innerHTML="";this.a=Math.round(Math.random()*10);return 1;case 1:this.elem("screenshot").innerHTML=""+this.a+"\n\n\n";this.elem("a").innerHTML=this.a;return 2;case 2:if(this.a%2==0){return 3}else{return 4}case 3:this.elem("screenshot").innerHTML=""+this.a+"\npáros\n\n";this.aktualis++;return 0;case 4:this.elem("screenshot").innerHTML=""+this.a+"\npáratlan\n\n";return 0}}}function feltcobj(a){debuggerlike.call(this,a);this.csinal=function(){switch(this.aktualis){case 0:this.elem("screenshot").innerHTML="<blink>_</blink>\n\n";this.a=Math.round(Math.random()*8-4);return 1;case 1:this.elem("screenshot").innerHTML=""+this.a+"\n\n";return 2;case 2:if(this.a>0){return 3}else{return 4}case 3:this.elem("screenshot").innerHTML=""+this.a+"\npozitív\n";return 4;case 4:if(this.a==0){return 5}else{return 6}case 5:this.elem("screenshot").innerHTML=""+this.a+"\nnulla\n";return 6;case 6:if(this.a<0){return 7}else{return 8}case 7:this.elem("screenshot").innerHTML=""+this.a+"\nnegatív\n";return 8}}}function ciklaobj(a){debuggerlike.call(this,a);this.csinal=function(){switch(this.aktualis){case 0:this.elem("screenshot").innerHTML="\n\n";this.elem("i").innerHTML="";this.elem("n").innerHTML="";this.szamki="";return 1;case 1:this.elem("screenshot").innerHTML="Meddig írjam ki? <blink>_</blink>\n\n";return 2;case 2:this.n=5;this.elem("n").innerHTML=this.n;this.elem("screenshot").innerHTML="Meddig írjam ki? "+this.n+"\n\n";return 3;case 3:this.i=1;this.elem("i").innerHTML=this.i;return 4;case 4:if(this.i<=this.n){return 5}else{return 7}case 5:this.szamki+=this.i+" ";this.elem("screenshot").innerHTML="Meddig írjam ki? "+this.n+"\n"+this.szamki+"\n";return 6;case 6:this.i++;this.elem("i").innerHTML=this.i;return 4;case 7:this.szamki+=".";this.elem("screenshot").innerHTML="Meddig írjam ki? "+this.n+"\n"+this.szamki+"\n";return 0}}}function ciklbobj(a){debuggerlike.call(this,a);this.csinal=function(){switch(this.aktualis){case 0:this.elem("szam").innerHTML="";this.elem("tipp").innerHTML="";this.elem("atlag").innerHTML="";this.elem("szampertipp").innerHTML="";return 1;case 1:this.szam=2;this.elem("szam").innerHTML=this.szam.toFixed(4);return 2;case 2:this.tipp=1;this.elem("tipp").innerHTML=this.tipp.toFixed(4);this.elem("szampertipp").innerHTML=(this.szam/this.tipp).toFixed(4);this.elem("tippminusz").innerHTML=Math.abs(this.tipp-this.szam/this.tipp).toFixed(4);this.elem("atlag").innerHTML=((this.tipp+this.szam/this.tipp)/2).toFixed(4);return 3;case 3:if(Math.abs(this.tipp-this.szam/this.tipp)>0.0001){return 4}else{return 5}case 4:this.tipp=(this.tipp+this.szam/this.tipp)/2;this.elem("tipp").innerHTML=this.tipp.toFixed(4);this.elem("szampertipp").innerHTML=(this.szam/this.tipp).toFixed(4);this.elem("tippminusz").innerHTML=Math.abs(this.tipp-this.szam/this.tipp).toFixed(4);this.elem("atlag").innerHTML=((this.tipp+this.szam/this.tipp)/2).toFixed(4);return 3;case 5:return 6}}}infoc.onloads.push(function(){infoc.ea02={szekvpeld:new szekvobj("szekv"),feltbpeld:new feltbobj("feltb"),feltcpeld:new feltcobj("feltc"),ciklapeld:new ciklaobj("cikla"),ciklbpeld:new ciklbobj("ciklb")}});