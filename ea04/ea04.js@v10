"use strict";infoc.onloads.push(function(){setInterval(function(){var c=$("szam10");var a=$("szam8");var b=$("szam2");var d=+c.innerHTML+1;c.innerHTML=d.toString(10);a.innerHTML=d.toString(8);b.innerHTML=d.toString(2)},500)});function kettesbe(){var b=parseInt($("kettesbe").value);var e=isNaN(b)?0:(b%128);var d="";for(var a=6;a>=0;--a){$("maradek"+a).innerHTML=e%(2<<a);var c=Math.floor(e/(1<<a));$("hanyados"+a).innerHTML=c;d=d+c;e=e%(1<<a)}while(d.length>1&&d[0]=="0"){d=d.substr(1)}$("kettes").value=d}function kettesbol(){var d=parseInt($("kettesbol").value);var f=isNaN(d)?"0":""+d;while(f.length<7){f="0"+f}var a=0;for(var c=6;c>=0;--c){var e=f[6-c]=="1"?1:0;var b=e<<c;a+=b;$("szamjegy"+c).innerHTML=e;$("szorzat"+c).innerHTML=b}$("tizes").value=a}function bitset(c,a,d){var b=$(c+a);b.innerHTML=d;b.className=(d?"egy":"nulla")}function bitget(d,a,c){var b=$(d+a);var e=b.innerHTML=="1"?1:0;b.className=(e?"egy":"nulla");return e}function toggle(a){var b=a.innerHTML=="1"?0:1;a.innerHTML=b;a.className=(b?"egy":"nulla")}function vagymuvelet(){for(var a=0;a<8;++a){bitset("vagyki",a,bitget("vagybea",a)|bitget("vagybeb",a))}}function esmuvelet(){for(var a=0;a<8;++a){bitset("eski",a,bitget("esbea",a)&bitget("esbeb",a))}}function xormuvelet(){for(var a=0;a<8;++a){bitset("xorki",a,bitget("xorbea",a)^bitget("xorbeb",a))}}function notmuvelet(){for(var a=0;a<8;++a){bitset("notki",a,1^bitget("notbea",a))}}infoc.onloads.push(vagymuvelet);infoc.onloads.push(esmuvelet);infoc.onloads.push(xormuvelet);infoc.onloads.push(notmuvelet);function fuggvobj(c){this.elem=function(d){return $(this.melyik+d)};this.jelol=function(){for(var d=1;this.elem(d)!=null;d++){classt_levesz(this.elem(d),"aktiv")}if(this.aktualis!=0){classt_feltesz(this.elem(this.aktualis),"aktiv")}};this.kovetkezo=function(){this.aktualis=this.csinal();if(this.elem(this.aktualis)==null){this.aktualis=0}this.jelol()};this.melyik=c;this.aktualis=0;this.jelol();this.show=function(d){d=this.elem(d);classt_levesz(d,"hidden");classt_feltesz(d,"visible")};this.hidenow=function(d){d=this.elem(d);classt_levesz(d,"visible");classt_feltesz(d,"hidden")};this.hide=function(e){var d=this;setTimeout(function(){d.hidenow(e)},1000)};this.csinal=function(){var d=this;switch(this.aktualis){case 0:this.elem("szam").innerHTML="";this.elem("eredm").innerHTML="";this.elem("mie").innerHTML="";this.elem("szorzat").innerHTML="";this.elem("i").innerHTML="";this.elem("vissza").innerHTML="";this.elem("screenshot").innerHTML="\n\n";this.show("mainstack");return 1;case 1:this.elem("screenshot").innerHTML="sz = 8\n\n";this.szam=8;this.elem("szam").innerHTML=this.szam;return 2;case 2:this.show("faktstack");this.mie=this.szam;this.elem("mie").innerHTML=this.elem("param").innerHTML=this.mie;return 3;case 3:this.szorzat=1;this.elem("szorzat").innerHTML=this.szorzat;this.i=0;return 4;case 4:if(this.i==0){this.i=2}this.elem("i").innerHTML=this.i;if(this.i<=this.mie){this.aktualis++;setTimeout(function(){d.kovetkezo()},200);return 5}else{return 6}case 5:this.szorzat*=this.i;this.elem("szorzat").innerHTML=this.szorzat;this.i++;this.elem("i").innerHTML=this.i;setTimeout(function(){d.kovetkezo()},200);return 4;case 6:this.vissza=this.szorzat;this.elem("vissza").innerHTML=this.vissza;return 7;case 7:this.hidenow("faktstack");this.eredm=this.vissza;this.elem("eredm").innerHTML=this.eredm;return 8;case 8:this.elem("screenshot").innerHTML="sz = "+this.szam+"\n"+this.szam+"! = "+this.eredm;return 9;case 9:this.hidenow("mainstack");return 0}};this.hidenow("faktstack");this.hidenow("mainstack");var b=this.elem("kovetkezo"),a=this;addEvent(b,"click",function(d){a.kovetkezo()})}infoc.onloads.push(function(){infoc.fuggvpeld=new fuggvobj("fuggv")});